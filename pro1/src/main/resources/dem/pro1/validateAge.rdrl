package dem.pro1;

import java.lang.Number;

rule "validateAge"
	dialect "mvel"
	when
		GeneralDetail( age == null || age < 18 )
	then
		Messages fact0 = new Messages();
		fact0.setMessage( "Age cannot be null or  below 18 yearfor this poli" );
		fact0.setMessageCode( 101 );
		insertLogical( fact0 );
end
